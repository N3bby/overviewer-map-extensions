{
  "websocketUrl": "ws://localhost:8080/ws",
  "removeCompass": true,
  "overlays": [
    {
      "world": "world",
      "render": "overworld",
      "overlayRender": "overworld-night",
      "enableWhenWorldIsLoaded":  true,
      "simulateDayNightCycle": true
    },
    {
      "world": "world",
      "render": "nether",
      "overlayRender": "nether-roof",
      "enableWhenWorldIsLoaded":  true
    }
  ],
  "dynamicPlayers": {
    "enabled": true,
    "dimensionMapping": [
      { "map": "world - overworld", "dimension": "minecraft:overworld"},
      { "map": "world - nether", "dimension": "minecraft:nether"},
      { "map": "world - end", "dimension": "minecraft:nether}"}
    ]
  }
}